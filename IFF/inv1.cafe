--> 2. fkrem1(p,r1,r2,r3)
open IFF .
	op a1 : -> Prin .
	op a2 : -> Prin .
	op a3 : -> Prin .
	op p : -> Protocol .
	op k : -> Key .
	op r : -> Rand .
	op r1 : -> Prin .
	op r2 : -> Prin .
	op r3 : -> Cipher .
  eq r3 \in ciphers(nw(p)) = true .
  eq a1 = intruder .
  eq r1 = a2 .
  eq r2 = a3 .
  eq k(r3) = k .
  eq r(r3) = r .
  eq p(r3) = a2 .
  eq k = k(intruder) .
  red inv1(p,a1,a2,a3,k,r) implies inv1(fkrem1(p,r1,r2,r3),a1,a2,a3,k,r) .
close

open IFF .
	op a1 : -> Prin .
	op a2 : -> Prin .
	op a3 : -> Prin .
	op p : -> Protocol .
	op k : -> Key .
	op r : -> Rand .
	op r1 : -> Prin .
	op r2 : -> Prin .
	op r3 : -> Cipher .
  eq r3 \in ciphers(nw(p)) = true .
  eq a1 = intruder .
  eq r1 = a2 .
  eq r2 = a3 .
  eq k(r3) = k .
  eq r(r3) = r .
  eq p(r3) = a2 .
  eq (k = k(intruder)) = false .
	eq a2 = intruder .
  eq enc(k,r,intruder) \in ciphers(nw(p)) = true .
  red inv2(p,k,r)
    implies inv1(p,a1,a2,a3,k,r) implies inv1(fkrem1(p,r1,r2,r3),a1,a2,a3,k,r) .
close

