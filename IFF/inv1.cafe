--> 5. sdrem(p,r1,r2,r3)
open IFF .
	op a1 : -> Prin .
	op a2 : -> Prin .
	op a3 : -> Prin .
	op p : -> Protocol .
	op k : -> Key .
	op r : -> Rand .
	op r1 : -> Prin .
	op r2 : -> Msg .
	eq chm?(r2) = true .
	eq (r2 \in nw(p)) = true .
	eq r1 = dst(r2) .

  	eq a1 = dst(r2) .
  	eq a3 = src(r2) .
  	eq k = k(dst(r2)) .
  	eq r = r(r2) .
  	eq a2 = dst(r2) .

  	eq dst(r2) = intruder .
	red inv1(p,a1,a2,a3,k,r) implies inv1(sdrem(p,r1,r2),a1,a2,a3,k,r) .
close

